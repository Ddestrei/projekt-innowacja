<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Personal_ID_Format_Validation</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(REGEX(Personal_ID_Number__c, &quot;^[0-9]{11}$&quot;))</errorConditionFormula>
    <errorDisplayField>Personal_ID_Number__c</errorDisplayField>
    <errorMessage>Personal ID number must consist of exactly 11 digits.</errorMessage>
</ValidationRule>
